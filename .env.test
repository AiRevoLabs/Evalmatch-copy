# Test Environment Configuration
NODE_ENV=test
PORT=5001

# Test Database (use separate test database if available)
# TEST_DATABASE_URL=postgresql://user:pass@localhost:5432/evalmatch_test
# If no test database, will use main DATABASE_URL

# Authentication bypass for testing
AUTH_BYPASS_MODE=true

# Firebase (use test project if available)
FIREBASE_PROJECT_ID=evalmatch-test
FIREBASE_SERVICE_ACCOUNT_KEY={"type":"service_account","project_id":"test"}

# AI Providers (use test keys or mock)
GROQ_API_KEY=test_groq_key
OPENAI_API_KEY=test_openai_key

# Logging
LOG_LEVEL=error

# Features
SERVE_STATIC=false
ENABLE_UPLOADS=true

# Security
SESSION_SECRET=test-secret-key-for-testing-only